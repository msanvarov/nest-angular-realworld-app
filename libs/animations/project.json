{
  "name": "animations",
  "projectType": "library",
  "sourceRoot": "libs/animations/src",
  "prefix": "starter",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/animations"],
      "options": {
        "jestConfig": "libs/animations/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/animations/**/*.ts",
          "libs/animations/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
